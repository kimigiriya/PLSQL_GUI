   <string notr="true">QMainWindow {
    background-color: #f5f7fa;
}

QWidget {
    font-family: 'Segoe UI', Arial, sans-serif;
    font-size: 16px;
}

QTabWidget::pane {
    border: 1px solid #d1d9e6;
    background-color: white;
    border-radius: 8px;
}

QTabBar::tab {
    background-color: #e8ebf0;
    padding: 10px 20px;
    margin-right: 2px;
    border-top-left-radius: 6px;
    border-top-right-radius: 6px;
    color: #5a6576;
    font-weight: 500;
}

QTabBar::tab:selected {
    background-color: white;
    color: #2c3e50;
    border-bottom: 2px solid #3498db;
    font-weight: 600;
}

QTabBar::tab:hover:!selected {
    background-color: #f0f3f7;
}

QGroupBox {
    border: 2px solid #e1e8ed;
    border-radius: 8px;
    margin-top: 10px;
    padding-top: 15px;
    font-weight: 600;
    color: #2c3e50;
    background-color: white;
}

QGroupBox::title {
    subcontrol-origin: margin;
    left: 15px;
    padding: 0 10px 0 10px;
    background-color: white;
}

QTableView {
    background-color: white;
    border: 1px solid #d1d9e6;
    border-radius: 6px;
    gridline-color: #e8ebf0;
    selection-background-color: #3498db;
    selection-color: white;
    alternate-background-color: #f8fafc;
}

QTableView::item {
    padding: 6px;
}

QTableView::item:selected {
    background-color: #3498db;
    color: white;
}

QHeaderView::section {
    background-color: #2c3e50;
    color: white;
    padding: 8px;
    border: none;
    font-weight: 600;
}

QPushButton {
    background-color: #3498db;
    border: none;
    border-radius: 6px;
    color: white;
    padding: 10px 20px;
    font-weight: 600;
    min-height: 36px;
    transition: all 0.3s ease;
}

QPushButton:hover {
    background-color: #2980b9;
    transform: translateY(-1px);
    box-shadow: 0 4px 12px rgba(52, 152, 219, 0.3);
}

QPushButton:pressed {
    background-color: #21618c;
    transform: translateY(0);
}

QPushButton:disabled {
    background-color: #bdc3c7;
    color: #7f8c8d;
}

QComboBox {
    border: 2px solid #d1d9e6;
    border-radius: 6px;
    padding: 8px 15px;
    background-color: white;
    min-height: 36px;
    selection-background-color: #3498db;
}

QComboBox:hover {
    border-color: #3498db;
}

QComboBox::drop-down {
    border: none;
    width: 30px;
}

QComboBox::down-arrow {
    image: url(down_arrow.png);
    width: 12px;
    height: 12px;
}

QComboBox QAbstractItemView {
    border: 1px solid #d1d9e6;
    border-radius: 6px;
    selection-background-color: #3498db;
    selection-color: white;
    background-color: white;
}

QLineEdit {
    border: 2px solid #d1d9e6;
    border-radius: 6px;
    padding: 8px 12px;
    background-color: white;
    min-height: 36px;
}

QLineEdit:focus {
    border-color: #3498db;
    box-shadow: 0 0 0 2px rgba(52, 152, 219, 0.2);
}

QLineEdit[placeholderText]:empty {
    color: #95a5a6;
}

QLabel {
    color: #2c3e50;
    font-weight: 500;
}

QFrame[frameShape="4"] {
    background-color: transparent;
    border: none;
}

QMenuBar {
    background-color: #2c3e50;
    color: white;
    padding: 5px;
}

QMenuBar::item {
    padding: 8px 15px;
    background-color: transparent;
    border-radius: 4px;
}

QMenuBar::item:selected {
    background-color: #3498db;
}

QStatusBar {
    background-color: #ecf0f1;
    color: #7f8c8d;
    padding: 5px;
}

QCheckBox {
    spacing: 8px;
    color: #2c3e50;
}

QCheckBox::indicator {
    width: 18px;
    height: 18px;
    border: 2px solid #d1d9e6;
    border-radius: 4px;
}

QCheckBox::indicator:checked {
    background-color: #3498db;
    border-color: #3498db;
    image: url(check.png);
}

